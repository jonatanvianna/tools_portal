option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "src.tools_adm.settings"
    "PYTHONPATH": "/opt/python/current/app/src/tools_adm:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: src/tools_adm/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "static/"